pipeline{
    agent any

    stages{
        stage('Dep install'){
            agent { docker { image 'nodejs'}}
            steps{
                sh 'cat /etc/os-release'                
                sh 'apt update'
                sh 'apt install sudo -y'
                sh 'sudo chown -R 995:993 "/.npm'
                sh'npm install'
            }
        }
    }
}